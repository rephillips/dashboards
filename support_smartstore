  <row>
    <panel>
      <title>smartstore components WARN/ERROR msgs (splunkd.log)</title>
      <table>
        <search>
          <query>index=_internal  host IN $host_list$ host IN $host$ source=*/splunkd.log component IN (S3Client,GCSClient,StorageInterface,CacheManager,CacheManagerHandler,KeyProviderManager)  | eval log_level=coalesce(log_level, "N/A"), component=coalesce(component, "N/A"), TIME=strftime(_time,"%H:%M:%S"), DATE=strftime(_time,"%Y/%m/%d"), FILENAME=mvindex(split(source,"/"), -1)
| search component=*
| search log_level IN ("CRITICAL","ERROR","FAILURE","WARN*")
| cluster showcount=true labelonly=t
| rename sourcetype as SOURCETYPE, log_level as LOG_LEVEL, component as COMPONENT
| stats earliest(_time) AS EARLIEST latest(_time) AS LATEST max(cluster_count) AS COUNT first(_raw) AS MESSAGE BY COMPONENT, LOG_LEVEL, SOURCETYPE, punct, cluster_label
| convert ctime(EARLIEST) ctime(LATEST)
| eval punct = replace(punct,"\"" ,"rocket\"")
| eval test = replace(punct,"rocket" ,"\ ")
| eval punct = replace(test," ", "")
| table COUNT EARLIEST LATEST SOURCETYPE LOG_LEVEL COMPONENT MESSAGE punct| sort - COUNT</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">3</option>
        <option name="dataOverlayMode">heatmap</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>bucket transaction</title>
      <table>
        <search>
          <query>index=_internal OR index=_audit   host IN $host_list$ host IN $host$ sourcetype=splunkd OR sourcetype=splunk_audit(local_bucket_upload completed) OR (local_bucket_upload started) OR (Error getting object) 
| eval receipt_bid=mvindex(split(name,"/"),0)."~".mvindex(split(name,"/"),4)
| eval cache_id_bid=mvindex(split(cache_id,"|"),1)
| eval my_bid=coalesce(cache_id_bid,receipt_bid) 
| transaction my_bid 
| table _time duration my_bid _raw</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">2</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
